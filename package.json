{
  "name": "file-sucker",
  "version": "0.1.3",
  "description": "Grab files from phone to laptop very over WIFI",
  "main": "index.js",
  "bin": {
    "file-sucker": "./dist/server.js"
  },
  "scripts": {
    "watch": "shadow-cljs watch client server",
    "serve": "http-server dist -s",
    "repl": "rlwrap shadow-cljs clj-repl",
    "build": "shadow-cljs clj-run build.main/build",
    "build-local": "shadow-cljs clj-run build.main/build-local",
    "page": "shadow-cljs clj-run build.main/page",
    "upload": "lumo -c cli/:src/ -m build.upload",
    "ln": "cd target && rm -f entry && ln -s ../entry"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mvc-works/file-sucker.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.18.0",
    "http-server": "^0.11.1",
    "shadow-cljs": "^2.6.6"
  },
  "dependencies": {
    "finalhandler": "^1.1.1",
    "formidable": "^1.2.1",
    "ip": "^1.1.5",
    "qrcode-terminal": "^0.12.0",
    "serve-static": "^1.13.2",
    "shortid": "^2.2.13"
  }
}
